<div class="dashdiv">

    <table class="table mt-0">
        <thead class="thead-light">
            <tr>
                <th>class Name</th>
                <th>Description</th>
                <th>Student</th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody class="table_body">
            <%if (cdata.length != null){ %>
                <% cdata.forEach(function(outvalue){ %>
                    <tr>
                        <td>
                            <%= outvalue.cname %>
                        </td>
                        <td>
                            <%= outvalue.desc %>
                        </td>
                        <td>
                            <%= outvalue.sname %>
                        </td>
                        <td>
                            <a id="editClass" href="/class/edit/<%= outvalue._id %>" data-id="<%= outvalue._id %>">Edit</a>
                        </td>
                        <td>
                            <a href="#" class="deleteClass" data-id="<%= outvalue._id %>">Delete</a>
                        </td>


                    </tr>
                    <% })}else{ %>
                        <tr>
                            <td colspan="6">No Record Found</td>

                        </tr>
                        <% } %>
        </tbody>
    </table>

    <div class="headbar">
        <nav class="nav navbarhead float-right">
            <a class="nav-link" href="/dashboard">Home</a>
            <a class="nav-link" href="/class/add">Add Class</a>

        </nav>
    </div>

    <% include ./partials/messages %>
</div>